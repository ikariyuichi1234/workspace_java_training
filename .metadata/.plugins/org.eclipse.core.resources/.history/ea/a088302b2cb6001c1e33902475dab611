package exam;

import java.util.Scanner;

public class Exam8 {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		String str = "";
		
		while(str.equals("yes")) {
			System.out.println("【yes】と入力しないでください");
			str = scan.nextLine();
		}

		scan.close();

	}

}