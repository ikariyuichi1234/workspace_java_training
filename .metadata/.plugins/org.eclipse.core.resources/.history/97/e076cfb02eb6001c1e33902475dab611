package exam;

import java.util.*;

public class Exam1 {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		int[] data = new int[10];
		
		for(int i = 0; i < data.length; i++) {
			data[i] = scan.nextInt();
			System.out.println(data[i] * 2);
		}
		
		scan.close();

	}

}
