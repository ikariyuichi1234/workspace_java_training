package exam;

public class Exam4 {
	public static void main(String[] args) {
		
		int price = 9260;
		int discountRate = 33;
		
		System.out.println("商品の金額は" + price + "円です");
		System.out.println("割引率は" + discountRate + "％です");
		System.out.println("割引される金額は" + (double)price * (double)discountRate / 100 + "円です");
	}

}
